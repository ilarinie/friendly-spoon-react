{
  "defaultSeverity": "error",
  "extends": [
      "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
          "singleQuote": true,
          "trailingComma": "all",
          "semiColons": true,
          "importName": false,
          "useTabs": false,
          "tabWidth": 2,
          "printWidth": 140
      }  
    ],
    "variable-name": false,
    "no-unused-variable": true
  },
  "rulesDirectory": []
}